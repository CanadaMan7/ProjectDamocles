	ID "RESEARCH_UNSC_HYPERSPACEDETECTION1_NAME"
	Value "Enhanced S.A.D.A.R."
StringInfo
	ID "RESEARCH_UNSC_HYPERSPACEGALAXYTRAVEL_DESCRIPTION"
