	ID "RESEARCH_UNSC_PLANETMODULEIMPROVEMENT_HANGARDEFENSE_NAME"
	Value "Hangar Flak Turrets"
StringInfo
	ID "RESEARCH_UNSC_PLANETMODULEIMPROVEMENT_CAPITALSHIPCONSTRUCTION_DESCRIPTION"
